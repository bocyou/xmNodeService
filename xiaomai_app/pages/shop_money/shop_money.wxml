<!--pages/shop_money/shop_money.wxml-->

<block wx:if="{{money>0}}">
    <view class="money">{{money}}<text>元</text></view>
    <view class="tip">已计入本周消费</view>
</block>
<block wx:elif="{{money==-1}}">
    <view class="tip-txt">正在识别价格……</view>
</block>
<block wx:else>
    <view class="tip-txt"></view>
</block>

