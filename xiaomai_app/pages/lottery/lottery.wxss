/* pages/lottery/lottery.wxss */

page{
    color:#717171;
    background-color: #f2f2f2;
}
.word-bar{
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding:10rpx 30rpx;


}
.word-bar .user{
    display:flex;
    align-items:center;

}
.word-bar .user .user-img{
    width:100rpx;
    height:100rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    overflow:hidden;
    border-radius: 26rpx;
    margin-bottom:46rpx;
}
.word-bar .right{
    border-bottom:1rpx solid #f5f5f7;
    flex:1;
    margin-left:36rpx;
    padding-bottom:46rpx;

}
.word-bar .right .user-name{
    color:#0d1722;
    font-size:30rpx;
    margin-bottom:6rpx;

}
.word-bar .right .word-info{
   color:#9e9fa6;
    font-size:24rpx;
}

.bottom-btn{
    position:fixed;
    bottom:0;
    left:0;
    width:100%;
}
.form-bet{
    width:100%;
    height:100%;
    position:fixed;
    top:0;
    left:0;
    background-color:rgba(0,0,0,.5);
    color:#717171;
    z-index: 100;
}
.form-bet .content{
    width:80%;
    margin:300rpx auto;
    background-color:#fff;
    border-radius: 8rpx;
    padding:30rpx;
}
.form-bet .content button{
    margin-top:60rpx;
}
.form-bet .title{
    text-align:center;
    line-height:60rpx;
    font-size:50rpx;
    margin-bottom:30rpx;
}
.form-bet .tip-err{
    font-size:24rpx;
    color: #bd3a29;
}
.rule{
    color:#717171;
    font-size:28rpx;
    padding:20rpx 30rpx;
    margin:20rpx 0 40rpx;
}
.rule .title{
    font-size:32rpx;
}
.placeholder{
    font-size:24rpx;
    color: #dcdcdc;
}

.user-bet-main .user-bet-title{
    font-size:32rpx;
    margin-bottom:20rpx;
}
.user-bet-main .content{
    display:flex;
    align-items: center;
    justify-content: flex-start;
    flex-wrap: wrap;
}
.user-bet-main .content .bar{
    width:80rpx;
    line-height:60rpx;
    text-align:center;
    background-color: #dfdfdf;
    margin:0 10rpx 10rpx;

}
.lucky-user-content{
    margin-bottom:0!important;
}

.lucky-user-content .title{
    line-height:100rpx;
    border-bottom:1px solid #f4f4f4;
    font-size:36rpx;
    padding:0 30rpx;
    font-weight: 500;

}
.lucky-user-content .title .text{
    font-size:26rpx;
    font-weight: normal;
    display: inline;
}
.lucky-user-content .title .text text{
    font-weight:bold;
    font-size:36rpx;
}
.lucky-user{
    display:flex;
    align-items: center;
    justify-content: space-around;
    flex-wrap: wrap;
    padding:30rpx 30rpx 0;

}
.lucky-user .user-info{
    width:120rpx;
    margin-bottom:16rpx;

}
.lucky-user .user-info .user-img{
    width:120rpx;
    height:120rpx;
    border-radius: 100px;
    background-size: cover;
    background-repeat:no-repeat;
    background-position: center;
}
.lucky-user .user-info .user-name{
    text-align:center;
    font-size:30rpx;
    margin:10rpx auto;
}
.lucky-user .user-info .word{
    font-size:28rpx;
    text-align:center;
}

.header-title{
  text-align:center;
    font-size:40rpx;
    color:#030405;
    background-color:#fff;
    padding:60rpx 0 30rpx;

}
.header-title .go-des{
    font-size:24rpx;
    color:#9e9fa6;

    display: inline-block;
}
.header-title .go-des text{
    text-decoration: underline;
}
.canvas{
    margin-bottom:40rpx;

}
.block-content{
    border-top:6rpx solid #f5f5f5;
    background-color:#fff;
    margin:30rpx 0;
}
.block-content .word-bar:last-child .right{
    border:none;
}
.block-content .title{
    line-height:100rpx;
    border-bottom:1px solid #f4f4f4;
    font-size:36rpx;
    padding:0 30rpx;
    font-weight: 500;
    color:#030405;
}
.block-content .content{
    padding:0 30rpx;
}
.content-tip{
    padding:60rpx 30rpx;
    font-size:26rpx;
    text-align: center;
    background-color:#fff;
}
.title-tip{
    font-size:40rpx;
    text-align:center;
}
.result-title{
    margin:60rpx auto;
}
.go-term-more{
    font-size:24rpx;
    text-align:right;
    color: #333d9f;
    display:block;
    background-color:#fff;
    border-top:2rpx solid #f5f5f5;
    line-height:100rpx;
    padding:0 30rpx;
    margin-bottom:30rpx;
}
.poor-title{
    padding:60rpx 0rpx 50rpx;
    text-align:center;
    margin:0rpx;
    background-color:#111;
   /* background-image: url('https://official-web.oss-cn-beijing.aliyuncs.com/mini_program/xiaomai/bg3.png');*/

}
.poor-title text{
    color:#fff;
    font-family:Monoton;
    font-size:130rpx;
    text-shadow: 0 0 10px #fff,
    0 0 20px  #fff,
    0 0 30px  #fff,
    0 0 40px  #FF1177,
    0 0 70px  #FF1177,
    0 0 80px  #FF1177,
    0 0 100px #FF1177,
    0 0 150px #FF1177;

}
.poor-title .tip{
    font-size:24rpx;
}
.go-rules{
 display: flex;
    justify-content: flex-end;
    font-size:24rpx;
    text-decoration: underline;
}

.week_words{
    font-size:30rpx;
    padding:0 30rpx;
    margin-top:30rpx;
    margin-bottom:30rpx;
    background-color:#fff;

}
.week_words .word-info{
    text-align: center;
    padding:60rpx 30rpx 40rpx;

}
.week_words .word-info .txt{
    font-family:Fette-Engschrift;
    font-size:80rpx;
    margin-bottom:20rpx;
}
.week_words .word-info .tip{
    font-size:24rpx;
}
.week_words .word-info .txt text{
    color:#FF1177;
}
.injection-btn{
    background-color: #dcdcdc;
    padding:30rpx 20rpx;
    border-radius:10rpx;
    font-size:26rpx;
    margin:30rpx;
    text-align: center;
    color:#030405;
}

.yan-btn{
    width:100%;
    height:110rpx;
    background-image:url("https://official-web.oss-cn-beijing.aliyuncs.com/mini_program/yan_btn.jpg");
    background-size:cover;
    background-position: center;
    background-repeat: no-repeat;
}
.yuce-btn{
    width:84%;
    margin:0 auto 30rpx;
    text-align:center;
    font-size:30rpx;
    line-height:80rpx;
    background-color:#000;
    color:#fff;
    border-radius:8rpx;
    box-shadow: 0 0 46rpx rgba(0,0,0,.2);
    -webkit-animation: neon1 .6s ease-in-out infinite alternate;
    -moz-animation: neon1 .6s ease-in-out infinite alternate;
    animation: neon1 .6s ease-in-out infinite alternate;

}

@-webkit-keyframes neon1 {
    from {
        transform: scale(1);
    }
    to {
        transform: scale(1.1);
    }
}

.font, .font span {
    transition: 2s;
    font-size:180rpx;

}

